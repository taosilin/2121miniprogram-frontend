/* components/searchBox/searchBox.wxss */
.container{
  width: 100%;
}
.searchHeader{
  width: 100%;
  height: 45px;
  border-bottom: 1px solid #000000;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.inputDiv{
  width:80%;
  margin-left: 22px;
  /* border: tomato 1px solid; */
}
.cancelIcon{
  width: 13.5px;
  padding: 17px;
  /* border: solid 1px blue; */
}
.searchIcon{
  width: 17.5px;
  margin-left: 17px;
  margin-right: 20px;
  /* border: 1px solid green; */
}
.searchHistory{
  width: 100%;
  height: 100%;
  position: absolute;
  overflow-y: scroll;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 10;
}
.z-height{
  width: 100%;
  overflow-y: scroll;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.historyTitle{
  width: 100%;
  font-size: 14px;
  color: #000000;
  box-sizing: border-box;
  padding: 25px 21px;
}
.historyList{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding: 19px 21px;
  box-sizing: border-box;
}
.historyItem{
  border-radius: 20px;
  font-size: 13px;
  color: #000000;
  background-color: #F7F7F7;
  padding: 11px 22px;
  margin-right: 15px;
  margin-bottom: 19px;
}
.clear{
  width: 40%;
  font-size: 14px;
  color: #000000;
  font-weight: bold;
  text-align: center;
  box-sizing: border-box;
  padding: 25px 0px;
}