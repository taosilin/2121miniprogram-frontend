/* components/searchBox/searchBox.wxss */
.container{
  width: 100%;
}
.searchHeader{
  width: 100%;
  height: 90rpx;
  border-bottom: 2rpx solid #000000;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.inputDiv{
  width:80%;
  margin-left: 44rpx;
  /* border: tomato 1px solid; */
}
.cancelIcon{
  width: 27rpx;
  padding: 34rpx;
  /* border: solid 1px blue; */
}
.searchIcon{
  width: 35rpx;
  margin-left: 34rpx;
  margin-right: 40rpx;
  /* border: 1px solid green; */
}
.searchHistory{
  width: 100%;
  height: 100%;
  position: absolute;
  overflow-y: scroll;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 9;
}
.z-height{
  width: 100%;
  height: 574rpx;
  overflow-y: scroll;
  background: #fff;
  position: absolute;
  z-index: 10;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.historyTitle{
  width: 100%;
  font-size: 28rpx;
  color: #000000;
  box-sizing: border-box;
  padding: 50rpx 42rpx;
}
.historyList{
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding: 38rpx 42rpx;
  box-sizing: border-box;
}
.historyItem{
  border-radius: 40rpx;
  font-size: 26rpx;
  color: #000000;
  background-color: #F7F7F7;
  padding: 22rpx 44rpx;
  margin-right: 30rpx;
  margin-bottom: 38rpx;
}
.clear{
  width: 40%;
  font-size: 28rpx;
  color: #000000;
  font-weight: bold;
  text-align: center;
  box-sizing: border-box;
  padding: 50rpx 0px;
}