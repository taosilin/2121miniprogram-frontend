/* pages/likeList/likeList.wxss */
.container{
  margin: 0px;
  padding: 0px;
}
.likeDiv{
  width: 100%;
  padding: 42rpx 44rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}
.likeItem{
  display: flex;
  flex-direction: row;
  position: relative;
  margin-bottom: 28rpx;
  border-radius: 16rpx;
  box-shadow: 0rpx 2rpx 12rpx #D9D9D9;
}
.imgDiv{
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 52rpx 0rpx;
  box-sizing: border-box;
}

.itemImg{
  width: 308rpx;
  /* margin-left: 36rpx;
  margin-top: 70rpx; */
}
.content{
  padding-top: 76rpx;
  margin-left: 26rpx;
}
.name{
  font-size: 28rpx;
  color: black;
}
.price{
  font-size: 34rpx;
  color: black;
  position: absolute;
  bottom: 36rpx;
}
.likeIcon{
  width: 30rpx;
  position: absolute;
  top: 22rpx;
  right: 24rpx;
}
.nothingDiv{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.noLikeImg{
  width: 299rpx;
  margin-top: 120rpx;
  margin-bottom: 64rpx;
}
.promptText{
  font-size: 28rpx;
  letter-spacing: 4rpx;
  color: #585858;
}